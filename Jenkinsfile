pipeline {
    agent any

    stages {
        stage('Hello2') {
            steps {
                echo 'Hello World2'
            }
        }
        stage('Build2') {
            steps {
                echo 'Building2'
            }
        }
        stage('Test2') {
            steps {
                echo 'Testing2'
            }
        }
        stage('Deploy2') {
            steps {
                echo 'Deploying2'
            }
        }
    }
}
